<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Copyright>2021-2024 EpicChain Lab's</Copyright>
    <AssemblyTitle>EpicChain.API</AssemblyTitle>
    <Version>1.0.0</Version>
    <Authors>EpicChain Lab's</Authors>
    <TargetFramework>net5.0</TargetFramework>
    <PackageId>EpicChain.API</PackageId>
    <PackageTags>EpicChain;EpicChain</PackageTags>
    <PackageProjectUrl>https://github.com/epicchainlabs/epicchain-api-csharp</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/epicchainlabs/epicchain-api-csharp.git</RepositoryUrl>
    <RootNamespace>EpicChain.API</RootNamespace>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <GenerateTargetFrameworkAttribute>false</GenerateTargetFrameworkAttribute>
    <Company>EpicChain Lab's</Company>
    <Description>EpicChain API for CSharp</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Google.Protobuf" Version="3.15.2" />
    <PackageReference Include="Grpc.Core" Version="2.35.0" />
    <PackageReference Include="EpicChain" Version="1.0.0-CI01194" />
  </ItemGroup>

</Project>
